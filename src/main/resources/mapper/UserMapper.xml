<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aaa.sujiexiche.sj.mapper.UserMapper">

    <select id="selectUser" resultType="com.aaa.sujiexiche.sj.entity.UserEntity">
        select * from sys_permission q,sys_user u where username=#{username} and q.u_id=u.u_id
    </select>

      <insert id="addUser">
          insert into sys_user()
      </insert>
</mapper>